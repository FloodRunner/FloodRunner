  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "SandboxRunner Debug",
      "skipFiles": ["<node_internals>\\**"],
      "program": "${workspaceFolder}\\SandboxRunner\\build\\index.js",
      "env": {
        "NODE_ENV": "DEV"
      },
      "cwd": "${workspaceFolder}\\SandboxRunner",
      "outFiles": ["${workspaceFolder}\\SandboxRunner\\build\\**\\*.js"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "API Debug",
      "skipFiles": ["<node_internals>\\**"],
      "program": "${workspaceFolder}\\api\\dist\\main.js",
      "env": {
        "NODE_ENV": "develop"
      },
      "cwd": "${workspaceFolder}\\api",
      "outFiles": ["${workspaceFolder}\\api\\dist\\**\\*.js"]
    }
  ]
}
