{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "allowJs": true,
    "outDir": "./build",
    "skipLibCheck": true,
    "rootDir": "./Puppeteer",
    "esModuleInterop": true,
    "inlineSourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false
  },
  "exclude": ["**/testscript*.ts", "build", "Puppeteer/src/floodTests/**", "Puppeteer/testResults/**"]
}
